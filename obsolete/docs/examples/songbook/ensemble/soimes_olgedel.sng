midiSuffixes: ["sa","t", "b", "satb"]

; text: |

  \begin{enumerate}
  
  \item
    \textbf{Sõimes õlgedel} Petlemmas /
    väike \textbf{Jeesus uinumas}. \\
    Valge \textbf{ingli tiib} tema juurde \textbf{viib} / 
    \textbf{kummarda}ma suurimat \textbf{armastust}.

  \item
    \textbf{Ema} Maria \textbf{põlvedel} /
    \textbf{on} nii \textbf{hea Jeesusel}. / 
    Valge \textbf{ingli tiib} ...
    

  \item
    \textbf{Keskel} Saaroni \textbf{rooside} /
    \textbf{magab} püha \textbf{lapsuke} / 
    Valge \textbf{ingli tiib} ...

  \end{enumerate}
  
  
