from lino.utils.instantiator import Instantiator, i2d

from django.db import models
projects = models.get_app('projects')
contacts = models.get_app('contacts')


project = Instantiator(projects.Project).build
note = Instantiator(projects.Note).build


def objects():
    oshz = contacts.Company.objects.get(name=u"Ã–SHZ Eupen")
    prj = project(name="Testprojekt",person=1,company=oshz)
    yield prj 
    yield note(project=prj,date=i2d(20091006),short="Programmierung")
    
    prj = project(name="Testprojekt",person=2,company=oshz)
    yield prj 
    yield note(project=prj,date=i2d(20091007),short="Anschauen")
        
